{
  "name": "square",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server/app.js",
    "client:dev": "webpack -w",
    "client:build": "webpack",
    "client:release": "webpack --config webpack.production.config.js",
    "server": "node server/app.js",
    "server:dev": "node-dev server/app.js",
    "server:test": "mocha ./server/tests/**/*-tests.js",
    "server:cover": "istanbul cover node_modules/mocha/bin/_mocha -- ./server/tests/**/*-tests.js",
    "client:test": "mocha ./client/js/tests/**/*-tests.js  --require babel-register",
    "client:cover": "istanbul cover node_modules/mocha/bin/_mocha -- ./client/js/tests/**/*-tests.js --require babel-register"
  },
  "engines": {
    "node": "^8.9.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "nanoid": "^2.1.1",
    "node-notifier": "^6.0.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bootstrap": "^3.3.7",
    "bootstrap-loader": "^3.0.4",
    "bootstrap-sass": "^3.4.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "file-loader": "^3.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "random-int": "^2.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "socket.io-client": "^2.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  }
